            height: auto;
        }

        div.background {
            background-image: url('../../Images/jääpeilipysty.jpg');
            background-size: cover;
            background-repeat: no-repeat;
            width: 100%;
            height: 1800px;
            text-align: center;
        }

        div.transbox {
            background-color: rgb(39,43,62); /*musta*/
            opacity: 0.7;
            width: 100%;
            height: 100%;
        }

        div.transbox2 {
            background-color: rgb(39,43,62); /*musta*/
            opacity: 0.9;
            width: 100%;
            height: 100%;
        }

        div.transbox p {
            font-weight: bold;
            color: rgb(39,43,62);
            font-size: 200px;
            width: 100%;
        }

        div.pallot {
            opacity: 0.8;
            text-align: center;
            box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);
            padding: 3%;
            width: 100%;
            height: 100%;
        }


        div.col-sm-3 {
            padding-top: 5%;
            text-align: center;
       /*     width: 100%;*/
        }

        p.Text {
            padding-top: 5px;
            padding-right: 5px;
            padding-bottom: 5px;
            padding-left: 5px;
        }

        .w3-myfont {
            font-family: "Comic Sans MS", /*cursive,*/, sans-serif;
            font-size: 45px;
            opacity: 0.9;
        }
    </style>
}


