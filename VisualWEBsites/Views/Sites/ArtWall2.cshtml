            opacity: 0.5;
            filter: alpha(opacity=60); /* For IE8 and earlier */
            width: 100%;
            height: 1920px;
        }

        div.transbox2 {
            /*margin: 5px;*/
            background-color: #000000; /*musta*/
            border: 1px solid black;
            opacity: 0.6;
            filter: alpha(opacity=60); /* For IE8 and earlier */
            width: 100%;
            height: 1920px;
            /*display: -webkit-flex;*/
            display: flex;
            -webkit-flex-direction: column;
            flex-direction: column;
        }

        div.col-sm-4 {
            /*margin-top: 1%;*/
            margin-left: 65%;
        }

        .square {
            width: 350px;
            height: 100px;
            margin: 3px;
            padding: 0;
            border: 4px solid black;
        }

        div.teksti {
            position: absolute; /*rivittää tekstin ruutuun*/
            /*width: 100%; */
            font-size: 60px;
            font-weight: bold;
            padding-left: 10%;
            text-align: center;
            color: black;
        }
    </style>
}

